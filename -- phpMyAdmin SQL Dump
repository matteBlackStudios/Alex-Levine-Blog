--
-- Table structure for table `wp_optionspage_fields`
--

CREATE TABLE `wp_optionspage_fields` (
  `id` int(11) NOT NULL,
  `wpc_sectionID` int(11) NOT NULL,
  `wpc_name` varchar(255) COLLATE utf8mb4_unicode_ci NOT NULL,
  `wpc_description` varchar(255) COLLATE utf8mb4_unicode_ci NOT NULL,
  `wpc_optionKey` varchar(255) COLLATE utf8mb4_unicode_ci NOT NULL,
  `wpc_type` varchar(255) COLLATE utf8mb4_unicode_ci NOT NULL,
  `modified` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP
) ENGINE=InnoDB AUTO_INCREMENT=7 DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_unicode_ci;

